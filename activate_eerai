scriptpath=$( cd -P -- "$(dirname -- "$(command -v -- "$1")")" && pwd -P )
if [ ! -d $scriptpath/venv/ ]; then echo Execute $scriptpath/bootstrap_environment.sh first!;
else . $scriptpath/venv/bin/activate; fi
